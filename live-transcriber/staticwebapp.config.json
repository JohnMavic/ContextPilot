{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["alloweduser"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?prompt=select_account&post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/worklets/*", "*.js", "*.css", "*.png", "*.jpg", "*.svg"]
  }
}
